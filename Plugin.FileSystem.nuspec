<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="3.0.0">
    <id>Xam.Plugin.FileSystem</id>
    <version>$version$</version>
    <title>File System Plugin for Xamarin and Windows</title>
    <authors>Alberto Fustinoni</authors>
    <owners>Alberto Fustinoni</owners>
    <licenseUrl>https://github.com/aftnet/FileSystemPlugin/blob/master/License.txt</licenseUrl>
    <projectUrl>https://github.com/aftnet/FileSystemPlugin</projectUrl>
    <iconUrl>https://github.com/Aftnet/FileSystemPlugin/raw/master/Icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>File System API abstraction</description>
    <summary>File System API abstraction</summary>
    <tags>xamarin, plugin, plugin for xamarin, android, xamarin.forms, ios, file system, storage, UWP</tags>
    <dependencies>
      <group targetFramework="netstandard1.4">
        <dependency id="NETStandard.Library" version="1.6.1" />
      </group>
      <group targetFramework="uap10.0">
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="6.0.4" />
      </group>
      <group targetFramework="net46">
	    <dependency id="System.IO.FileSystem" version="4.3.0" />
      </group>
      <group targetFramework="MonoAndroid10">
      </group>
      <group targetFramework="Xamarin.iOS10">
      </group>
      <group targetFramework="Xamarin.Mac20">
      </group>
    </dependencies>
  </metadata>
  <files>
    <!--Standard-->
    <file src="Plugin.FileSystem\bin\$configuration$\netstandard1.3\Plugin.FileSystem.*" target="lib\netstandard1.3" />
    <!--UWP-->
    <file src="Plugin.FileSystem.UWP\bin\$configuration$\Plugin.FileSystem.*" target="lib\uap10.0" />
    <!--Windows Desktop-->
    <file src="Plugin.FileSystem.Net46\bin\$configuration$\Plugin.FileSystem.*" target="lib\net46" />
    <!--Android-->
    <file src="Plugin.FileSystem.Android\bin\$configuration$\Plugin.FileSystem.*" target="lib\MonoAndroid10" />
    <!--iOS-->
    <file src="Plugin.FileSystem.iOS\bin\$configuration$\Plugin.FileSystem.*" target="lib\Xamarin.iOS10" />
    <!--macOS-->
    <file src="Plugin.FileSystem.macOS\bin\$configuration$\Plugin.FileSystem.*" target="lib\Xamarin.Mac20" />
    <!--Core-->
    <file src="Plugin.FileSystem.NetCore\bin\$configuration$\netcoreapp2.0\Plugin.FileSystem.*" target="lib\netcoreapp2.0" />
  </files>
</package>